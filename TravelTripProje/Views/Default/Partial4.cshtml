@model List<TravelTripProje.Models.Sınıflar.Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {

        <div class="bot-img">
            <img src="@item.BlogImage"style="width:125px;height:60px" alt="" />
        </div>
        <div class="bot-desc">
            <p>@item.Aciklama.Substring(0,75)...</p>
        </div>
        <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")
</div>